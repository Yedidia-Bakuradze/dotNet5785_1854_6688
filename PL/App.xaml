<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PL"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:CallTypesCollection x:Key="CallTypesCollectionKey"/>
        <local:CallStatusCollection x:Key="CallStatusCollectionKey"/>
        <local:UserRoleCollection x:Key="UserRoleCollectionKey"/>
        <local:TypeOfRangeCollection x:Key="TypeOfRangeCollectionKey"/>
        <local:ConvertIntToReadOnlyTrue x:Key="ConvertIntToReadOnlyTrueKey"/>
        <local:ConvertStatusToReadOnlyTrue x:Key="ConvertStatusToReadOnlyTrueKey"/>
        <local:ConvertBooleanToVisibility x:Key="ConvertBooleanToVisibilityKey"/>
    </Application.Resources>
</Application>
